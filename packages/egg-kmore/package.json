{
  "name": "egg-kmore",
  "author": "waiting",
  "version": "0.13.1",
  "description": "egg plugin of kmore",
  "eggPlugin": {
    "name": "kmore"
  },
  "keywords": [
    "midway",
    "egg",
    "eggPlugin",
    "egg-plugin",
    "knex",
    "kmore",
    "typescript",
    "ts"
  ],
  "bin": {},
  "browser": "",
  "es2015": "",
  "main": "./index.cjs.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "bugs": {
    "url": "https://github.com/waitingsong/egg-kmore/issues"
  },
  "homepage": "https://waitingsong.github.io/kmore/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/waitingsong/egg-kmore.git"
  },
  "license": "MIT",
  "dependencies": {
    "@waiting/shared-types": "3",
    "kmore": "^0.13.1",
    "kmore-cli": "^0.13.1"
  },
  "devDependencies": {
    "cross-env": "6",
    "egg": "2",
    "knex": "^0.20.0",
    "pg": "^7.11.0"
  },
  "peerDependencies": {
    "knex": ">=0.15.0",
    "typescript": "3"
  },
  "engines": {
    "node": ">=10.16.0"
  },
  "files": [
    "config",
    "lib",
    "agent.js",
    "agent.d.ts",
    "app.js",
    "app.d.ts",
    "index.js",
    "index.cjs.js",
    "index.cjs.js.map",
    "index.d.ts"
  ],
  "scripts": {
    "build": "npm run tsc && npm run rp",
    "devbuild": "npm run tsc && npm run rp",
    "clean": "rm -rf dist/* .vscode/.tsbuildinfo .vscode/.tsbuildinfo.* ",
    "cov": "cross-env TS_NODE_PROJECT=test/tsconfig.json nyc mocha",
    "lint": "eslint --fix --cache {src,test}/**/*.ts",
    "lint:nofix": "eslint --cache {src,test}/**/*.ts",
    "rp": "rollup -c rollup.config.js",
    "rp:w": "npm run tsc:w | rollup -wc rollup.config.js",
    "test": "cross-env TS_NODE_PROJECT=test/tsconfig.json mocha --opts test/mocha.opts",
    "test:browser": "cd .config && npm run browser && cd ..",
    "tsc": "tsc -p tsconfig.json",
    "tsc:w": "tsc -p tsconfig.json -w"
  },
  "nyc": {
    "include": [
      "src/**/*.ts"
    ],
    "exclude": [
      ".githooks",
      "dist",
      "node_modules*",
      "src/bin",
      "src/domain.ts",
      "src/interface.ts",
      "test*",
      "**/node_modules*",
      "**/*.d.ts",
      "**/*.js"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "text-summary",
      "json",
      "html"
    ],
    "all": true
  }
}
